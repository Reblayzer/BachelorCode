{
  "ConnectionStrings": {
    "Default": "Data Source=../storage.db"
  },
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/auth/{**catchAll}"
        }
      },
      "connect-route": {
        "ClusterId": "linking-cluster",
        "Match": {
          "Path": "/api/connect/{**catchAll}"
        }
      },
      "connections-route": {
        "ClusterId": "linking-cluster",
        "Match": {
          "Path": "/api/connections/{**catchAll}"
        }
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "identity-service": {
            "Address": "https://localhost:7166/"
          }
        }
      },
      "linking-cluster": {
        "Destinations": {
          "linking-service": {
            "Address": "https://localhost:7030/"
          }
        }
      }
    }
  }
}
