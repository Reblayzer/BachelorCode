{
  "name": "storageconnector-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm:dev:frontend\" \"npm:dev:identity\" \"npm:dev:linking\"",
    "dev:frontend": "vite",
    "dev:identity": "cd ../services/IdentityService/Api && dotnet run --launch-profile https",
    "dev:linking": "cd ../services/LinkingService/Api && dotnet run --launch-profile https",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "classnames": "^2.5.1",
    "react": "^19.2.0",
    "react-data-table-component": "^7.7.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  }
}